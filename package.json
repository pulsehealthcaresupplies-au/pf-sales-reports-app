{
  "name": "pulse-healthcare-sales-reports",
  "version": "1.0.0",
  "description": "Sales Reports Dashboard for Pulse Healthcare Supplies",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3004",
    "build": "next build",
    "start": "next start -p 3004",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.sales-reports.yml && graphql-codegen --config codegen.auth.yml",
    "codegen:sales-reports": "graphql-codegen --config codegen.sales-reports.yml",
    "codegen:auth": "graphql-codegen --config codegen.auth.yml",
    "codegen:watch": "graphql-codegen --config codegen.sales-reports.yml --watch",
    "codegen:sync": "npm run codegen"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@apollo/client-integration-nextjs": "^0.14.0",
    "@apollo/experimental-nextjs-app-support": "^0.14.3",
    "@heroui/react": "latest",
    "@heroui/styles": "latest",
    "@heroui/theme": "latest",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.23.25",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.6",
    "lucide-react": "^0.555.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.5.1",
    "rxjs": "^7.8.2",
    "sonner": "^1.7.4",
    "tailwind-merge": "latest",
    "tailwindcss-animate": "latest"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-react-apollo": "^4.3.4",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-config-next": "latest",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "ws": "^8.19.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
